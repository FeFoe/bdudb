<!doctype html>
<html ng-app="bduApp">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!--for sharing on facebook and others-->
    <meta property="og:url" content="https://members.debating.de" />
    <meta property="og:type" content="web app" />
    <meta property="og:title" content="BDU Members Area" />
    <meta property="og:description" content="Internal club management of the Berlin Debating Union" />
    <meta property="og:image" content="https://members.debating.de/images/bdudb_landing-1024x682.png" />

    <title>BDU Members Area</title>

    <!-- load javascripts -->

    <!-- jQuery -->
    <script src="https://code.jquery.com/jquery-2.1.3.min.js"></script>

    <!-- lodash -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/lodash.js/4.17.4/lodash.min.js"></script>

    <!-- tether.io -->
    <script src="https://cdn.rawgit.com/HubSpot/tether/v1.3.4/dist/js/tether.min.js"></script>

    <!-- angular -->
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.0/angular.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.0/angular-route.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.0/angular-resource.min.js"></script>

    <!--Angular-Confirm-->
    <script src="/javascripts/angular-confirm-master/dist/angular-confirm.min.js"></script>
    <link rel="stylesheet" href="javascripts/angular-confirm-master/dist/angular-confirm.min.css">

    <!-- ngDialog -->
    <script src="/javascripts/ng-dialog/js/ngDialog.js"></script>
    <link rel="stylesheet" href="javascripts/ng-dialog/css/ngDialog.min.css">
    <link rel="stylesheet" href="javascripts/ng-dialog/css/ngDialog-theme-default.min.css">

    <!-- ng-file-upload -->
    <script src="/javascripts/ng-file-upload/ng-file-upload-shim.min.js"></script>
    <script src="/javascripts/ng-file-upload/ng-file-upload.min.js"></script>

    <!-- ng-img-crop -->
    <script src="/javascripts/ng-img-crop/js/ng-img-crop.js"></script>

    <!-- bootstrap -->
    <link rel="stylesheet" href="stylesheets/bootstrap/4.0.0-alpha.6/css/bootstrap.css" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.1.1.slim.min.js" integrity="sha384-A7FZj7v+d/sdmMqp/nOQwliLvUsJfDHW+k9Omg/a/EheAdgtzNs3hpfag6Ed950n" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js" integrity="sha384-DztdAPBWPRXSA/3eYEEUWrWCy7G5KFbe8fFjk5JAIxUYHKkDx6Qin1DkWx51bBrb" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/js/bootstrap.min.js" integrity="sha384-vBWWzlZJ8ea9aCX4pEW3rVHjgjt7zpkNpZk+02D9phzyeVkE+jo0ieGizqPLForn" crossorigin="anonymous"></script>

    <!-- fontawesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">

    <!--css-loader-->
    <link rel="stylesheet" href="stylesheets/loader.css" />

    <link href="https://fonts.googleapis.com/css?family=Montserrat:200,300,400,500" rel="stylesheet">

    <!--moment js-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.18.1/moment.min.js"></script>

    <!--moment precise range-->
    <script src="/javascripts/moment-precise-range/moment-precise-range.js"></script>

    <!--highcharts-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/highstock/5.0.12/highstock.js"></script>
    <script src="https://code.highcharts.com/modules/exporting.js"></script>
    <script src="/javascripts/highcharts/themes/dark_unica.js"></script>

    <!--OWN FILES-->
    <script src="/javascripts/bduApp.js"></script>
    <script src="/javascripts/resourceFactories.js"></script>
    <script src="/javascripts/successService.js"></script>
    <script src="/javascripts/controllers/rankingCtrl.js"></script>
    <script src="/javascripts/controllers/dashboardCtrl.js"></script>
    <script src="/javascripts/controllers/financeCtrl.js"></script>
    <script src="/javascripts/controllers/tournamentsCtrl.js"></script>

    <link rel="stylesheet" href="stylesheets/style.css">
    <link rel="stylesheet" href="stylesheets/inline_help_style.css">
    <link rel="stylesheet" href="stylesheets/landing.css">
    <link rel="stylesheet" href="stylesheets/dashboard.css">

</head>
<body>

<!--++++++++++++++++++++++++++ NAVBAR ++++++++++++++++++++++++++-->

<nav class="navbar navbar-inverse bg-inverse navbar-fixed-top navbar-toggleable-md bg-faded">
    <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <i class="fa fa-bars fa-lg" aria-hidden="true"></i>
    </button>
    <a class="navbar-brand" href="#/profile">
        <img class="nav-logo" ng-src="http://debating.de/BDUDBdev/bdu_white_logo.png" onerror="this.src='images/bdu_quad.png'" alt=""/>
    </a>
    <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav mr-auto">
            <li class="nav-item" ng-show="authenticated">
                <a href="/#/profile" class="nav-link hidden-sm-down">Profile</a>
                <a href="/#/profile" class="nav-link hidden-md-up" data-toggle="collapse" data-target=".navbar-collapse">Profile</a>
            </li>
            <li class="nav-item" ng-show="authenticated">
                <a href="/#/tournaments" class="nav-link hidden-sm-down">
                    Tournaments
                    <span ng-show="user.new_tournament_count > 0" class="count">{{user.new_tournament_count}}</span>
                </a>
                <a href="/#/tournaments" class="nav-link hidden-md-up" data-toggle="collapse" data-target=".navbar-collapse">Tournaments</a>
            </li>
            <li class="nav-item" ng-show="authenticated">
                <a href="/#/anmeldungen" class="nav-link hidden-sm-down">Reg Overview</a>
                <a href="/#/anmeldungen" class="nav-link hidden-md-up" data-toggle="collapse" data-target=".navbar-collapse">Reg Overview</a>
            </li>
            <li class="nav-item" ng-show="authenticated">
                <a class="nav-link hidden-sm-down" href="/#/ranking">Ranking</a>
                <a class="nav-link hidden-md-up" href="/#/ranking" data-toggle="collapse" data-target=".navbar-collapse">Ranking</a>
            </li>
            <li class="nav-item" ng-show="authenticated">
                <a class="nav-link hidden-sm-down" href="/#/dashboard">Dashboard</a>
                <a class="nav-link hidden-md-up" href="/#/dashboard" data-toggle="collapse" data-target=".navbar-collapse">Dashboard</a>
            </li>
            <li ng-show="istVorstand" class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="http://example.com" id="navbarDropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    Vorstand
                </a>
                <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                    <a class="dropdown-item hidden-sm-down" href="/#/vorstand">Create Tournament</a>
                    <a class="dropdown-item hidden-md-up" href="/#/vorstand" data-toggle="collapse" data-target=".navbar-collapse">Create Tournament</a>
                    <a class="dropdown-item hidden-sm-down" href="/#/finances">Club Members</a>
                    <a class="dropdown-item hidden-md-up" href="/#/finances" data-toggle="collapse" data-target=".navbar-collapse">Club Members</a>
                </div>
            </li>
        </ul>

        <ul class="navbar-nav my-2 my-lg-0" ng-show="authenticated">
            <li class="nav-item">
                <a href="/#/profile" class="hidden-md-down nav-link navbar-brand">
                    <img class="nav-image" ng-src="http://debating.de/BDUDBdev/userpics/{{user.image}}" onerror="this.src='images/bdu_quad.png'" alt=""/>
                </a>
            </li>
            <li class="nav-item hidden-sm-down">
                <span class="navbar-text" style="margin-right: 15px">Signed in as {{user.vorname}}</span>
            </li>
            <li class="nav-item">
                <!--SCHULDEN-->
                <span class="help-tip hidden-md-down" style="margin-right: 15px">
                    <span class="navbar-text currencySign" data-monetary-amount="{{personnelDebt | currency:'€'}}"></span>
					<p class="navbar-text">This is how much you currently owe the BDU.</p>
				</span>
            </li>

            <li class="nav-item">
                <a href="/#/" class="hidden-sm-down nav-link" ng-click="signout()"><i class="fa fa-sign-out" aria-hidden="true"></i> Logout</a>
                <a href="/#/" class="hidden-md-up visible-sm nav-link" data-toggle="collapse" data-target=".navbar-collapse" ng-click="signout()"><i class="fa fa-sign-out" aria-hidden="true"></i> Logout</a>
            </li>
        </ul>

        <ul class="nav navbar-nav navbar-right" ng-show="!authenticated">
            <li><a href="/#/" class="hidden-sm-down nav-link"><i class="fa fa-sign-in" aria-hidden="true"></i> Login</a></li>
            <li><a href="/#/" class="hidden-md-up nav-link" data-toggle="collapse" data-target=".navbar-collapse"><i class="fa fa-sign-in" aria-hidden="true"></i> Login</a></li>
            <li><a href="/#/signup" class="hidden-sm-down nav-link"><i class="fa fa-user-circle" aria-hidden="true"></i> Signup</a></li>
            <li><a href="/#/signup" class="hidden-md-up nav-link" data-toggle="collapse" data-target=".navbar-collapse"><i class="fa fa-user-circle" aria-hidden="true"></i> Signup</a></li>
        </ul>
    </div>
</nav>

<div ng-view class="content"></div>


<!-- LINK TO BUG REPORT PAGE -->
<footer class="footer">
    <div class="row">
        <div class="col hidden-md-down">
            © Berlin Debating Union e.V., 2017
        </div>
        <div class="col">
            <a href="/#/bugReport">Report a Problem<span class="hidden-md-down"> / Give feedback</span></a>
        </div>
        <div class="col photo-credit">
            photo credit: <a target="_blank" href="https://www.flickr.com/photos/jaehyunlee/">Jaehyun Lee</a>, <a target="_blank" href="https://www.flickr.com/photos/jaehyunlee/33543411320/">"Museum Island"</a> is licensed under <a target="_blank" href="https://creativecommons.org/licenses/by/2.0/">CC BY 2.0</a>
        </div>
    </div>
</footer>

<!-- SNACKBARS -->

<strong id="snackbarSuccess" class="alert alert-success"></strong>
<strong id="snackbarError" class="alert alert-danger"></strong>

<!--LOADER-->
<div ng-class="{'overlay': loader}"></div>
<div ng-show="loader" class="showbox">
    <div class="loader">
        <svg class="circular" viewBox="25 25 50 50">
            <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10"/>
        </svg>
    </div>
</div>


</body>
</html>
